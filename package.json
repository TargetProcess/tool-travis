{
  "name": "travisci-build-tool",
  "version": "1.0.0",
  "description": "Travis CI tool for BuildBoard",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start --name=\"travis\" --node-args=\"--harmony_destructuring\"  src/index.js",
    "dev": "pm2 --watch --no-daemon start --name=\"travis\" --node-args=\"--harmony_destructuring\"  src/index.js",
    "test": "mocha --harmony_destructuring test/**/*.js"
  },
  "author": "Taucraft",
  "license": "ISC",
  "dependencies": {
    "buildboard-tool-bootstrap": "github:buildboard/tool-bootstrap",
    "lodash": "^3.10.1",
    "thunkify-wrap": "^1.0.4",
    "travis-ci": "^2.0.3"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "pm2": "^0.15.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildboard/citool-travis.git"
  },
  "bugs": {
    "url": "https://github.com/buildboard/citool-travis/issues"
  },
  "homepage": "https://github.com/buildboard/citool-travis#readme",
  "directories": {
    "test": "test"
  }
}
